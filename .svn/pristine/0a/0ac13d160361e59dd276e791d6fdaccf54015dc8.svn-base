    <body> 
	
		<div class="detailTop">
			<div class="yue"><p>账户余额：</p><!--<p><a href="#" class="cash">提现</a></p>--></div>
			<div>￥$resultMap.get("totalReward")</div>
		</div>
		<ul class="detailList">
	##foreach( $product in $resultMap.keySet())
	#foreach( $productdetail in ${resultMap.get("orderList")})
			<li>
			<div>
		#if(${productdetail.orderType}==1)
				<p class="air">订机票</p>
